<div class="panel panel-default">
  <!-- Default panel contents -->
  <div class="panel-heading"><h3 class="panel-title">User Management</h3></div>
  <div class="panel-body">
    <dl class="dl-horizontal">
      <dt>Name</dt>
      <dd>Name from Auth0 service</dd>
      <dt>Email</dt>
      <dd>Email Address from Auth0 service</dd>
      <dt>Is Admin</dt>
      <dd>When checked, this user has access to the Admin portions of the application.</dd>
    </dl>
  </div>
  <table class="table table-striped table-condensed">
    <thead>
      <tr>
        <th>Name</th>
        <th>Email</th>
        <th>Email Verifed?</th>
        <th>Is Admin</th>
        <th>&nbsp;</th>
      </tr>
    </thead>
    <tbody>
  <%= render :partial => 'user', :collection => @users %>
    </tbody>
  </table>
</div>
