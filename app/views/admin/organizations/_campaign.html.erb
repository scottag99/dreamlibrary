<tr>
  <td>
    <%= link_to campaign.name, [:admin, campaign.organization, campaign] %>
  </td>
  <td><%= campaign.deadline %></td>
  <td>
    <%= link_to "Edit", edit_admin_organization_campaign_url(campaign.organization, campaign), class: "btn btn-warning btn-sm" %>
    <%= link_to "Obliterate", admin_organization_campaign_url(campaign.organization, campaign),  method: :delete, class: "btn btn-danger btn-sm", data: {confirm: 'Are you sure?'} %>
  </td>
</tr>
